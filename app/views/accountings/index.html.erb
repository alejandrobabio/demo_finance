<p id="notice"><%= notice %></p>

<h1>Listing Accountings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Commodity</th>
      <th>Book</th>
    </tr>
  </thead>

  <tbody>
    <% @accountings.each do |accounting| %>
      <tr>
        <td><%= accounting %></td>
        <td><%= accounting.commodity %></td>
        <td><%= accounting.book %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accounting', new_accounting_path %>
